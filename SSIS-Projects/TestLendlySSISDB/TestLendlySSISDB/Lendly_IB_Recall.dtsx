using System;
using System.Data;
using Microsoft.SqlServer.Dts.Pipeline.Wrapper;
using Microsoft.SqlServer.Dts.Runtime.Wrapper;
using System.Collections.Generic;
using RGS.ArtivaGeneralPurpose.Data.Records;
using FileHelpers;
using System.Data.Odbc;
#endregion

[Microsoft.SqlServer.Dts.Pipeline.SSISScriptComponentEntryPointAttribute]
public class ScriptMain : UserComponent
{
    private bool _exceptionReportCreated = false, _headerAdded = false;
    private Dictionary<string, Tuple<string, string, string, bool>> _recallInformation; 
    OdbcConnection conn;

    public override void PreExecute()
    {
        base.PreExecute();
        conn = new OdbcConnection(Connections.ArtivaConnection.ConnectionString);
        conn.Open();
    }

    public override void PostExecute()
    {
        base.PostExecute();       

        if (conn != null)
        {
            conn.Close();
            conn.Dispose();
        }
    }

    public override void Input0_ProcessInputRow(Input0Buffer Row)
    {
        bool accountFound = false;
        string artivaID = "";

        #region SQL Query
        string query = @"SELECT top 1
                                ARACID                             
                                FROM 
                                    %STARTTABLE ARCLIENT
                                    INNER JOIN ARACCOUNT ON ARCLID = ARACCLTID                                
                                WHERE  
                                    ARACCOUNT.ARACCLACCT = '" + Row.ClientReferenceNumber + @"'";

        using (var command = new OdbcCommand(query, conn))
        {
            if (conn.State != ConnectionState.Open)
                conn.Open();

            command.CommandTimeout = 300;
            using (var reader = command.ExecuteReader())
            {
                if (reader.Read())
                {
                    accountFound = true;
                    artivaID = reader.GetDecimal(0).ToString();
                }
            }
        }
        #endregion

        if (!accountFound)
        {
            _exceptionReportCreated = true;
            ExceptionReportBuffer.AddRow();
            ExceptionReportBuffer.Data = "ACCOUNT NOT FOUND IN ARTIVA, Client Reference Number from file:  " + Row.ClientReferenceNumber;
            return;
        }
        
        Add201Record(artivaID, Row.RecallCode, Row.RecallReason);
        AddCloseRecord(artivaID);          
    }     

    private void Add201Record(string artivaID, string obRecallCode, string statusDescription)
    {
        var rec201 = new ClientRecallInfo_201
        {
            AccountId = artivaID.ToString(),
            DateOfRecall = DateTime.Today,
            RecallCode = obRecallCode,
            RecallReason = statusDescription,
            ClientRecallFileName = Variables.currentFile
        };

        AddMaintenanceRow(rec201);
    }
    
    private void AddCloseRecord(string artivaID)
    {

        var recC = new CloseAccount_C
        {
            AccountId = artivaID,
            CloseCode = "RETURN"
        };

        AddMaintenanceRow(recC);
    }

    private void AddMaintenanceRow<T>(T record) where T : GeneralPurposeRecordBase
    {
        var recordList = new List<T>();
        recordList.Add(record);
        var recordString = CommonEngine.WriteString<T>(recordList).Replace("\r\n", "");
        MaintenanceBuffer.AddRow();
        MaintenanceBuffer.RowData = recordString.ToUpper();
    }
}]]></arrayElement>
