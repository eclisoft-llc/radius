            <component
              refId="Package\Data Flow Task\ODBC Source"
              componentClassID="{A77F5655-A006-443A-9B7E-90B6BD55CB84}"
              contactInfo="ODBC Source;Connector for Open Database Connectivity (ODBC) by Attunity; Attunity Ltd.; All Rights Reserved; http://www.attunity.com;6"
              description="ODBC Source"
              name="ODBC Source"
              usesDispositions="true"
              version="2">
              <properties>
                <property
                  dataType="System.String"
                  description="The name of the table to be fetched."
                  expressionType="Notify"
                  name="TableName"></property>
                <property
                  dataType="System.String"
                  description="The SQL command to be executed."
                  expressionType="Notify"
                  name="SqlCommand">SELECT   
     ARACID
    ,ARACCANCID   
    ,ARACCLACCT 
    ,ARRELATIONSHIP.ARRELSTATUSID AS Status_Reason_Codes
    ,CASE 
        WHEN ARRELATIONSHIP.ARRELSTATUSID IN ('DECEASED') THEN 'Deceased'
        WHEN ARRELATIONSHIP.ARRELSTATUSID IN ('BANKRUPT') THEN 'Bankrupt'
        WHEN ARRELATIONSHIP.ARRELSTATUSID IN ('CEASE') THEN 'Cease Communications'
        WHEN ARRELATIONSHIP.ARRELSTATUSID IN ('MILDEPLY') THEN 'Active in the Military'
        WHEN ARRELATIONSHIP.ARRELSTATUSID IN ('LIT') THEN 'Litigous Customer'
        WHEN ARRELATIONSHIP.ARRELSTATUSID IN ('CEASCALL') THEN 'Cease Calls'        
     END AS Status_Reason
    ,ARACCOUNT.ARACCANCID AS Cancel_Reason_Codes
    ,CASE 
        WHEN ARACCOUNT.ARACCANCID IN ('DECEASED') THEN 'Responible Party is Deceased'
        WHEN ARACCOUNT.ARACCANCID IN ('BNK') THEN 'Bankruptcy Chapter Unknown'
        WHEN ARACCOUNT.ARACCANCID IN ('CEASE') THEN 'Case and Desist'
        WHEN ARACCOUNT.ARACCANCID IN ('SCRA') THEN 'SCRA/Military'
        WHEN ARACCOUNT.ARACCANCID IN ('LIT') THEN 'Litigant Scrub Hit'
        WHEN ARACCOUNT.ARACCANCID IN ('BNK7') THEN 'Bankruptcy Chapter 7'
        WHEN ARACCOUNT.ARACCANCID IN ('BNK11') THEN 'Bankruptcy Chapter 11'
        WHEN ARACCOUNT.ARACCANCID IN ('BNK13') THEN 'Bankruptcy Chapter 13'
