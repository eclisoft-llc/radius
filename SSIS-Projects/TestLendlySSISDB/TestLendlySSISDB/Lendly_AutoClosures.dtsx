         */
    }

    /// <summary>
    /// This method is called once for every row that passes through the component from Input0.
    ///
    /// Example of reading a value from a column in the the row:
    ///  string zipCode = Row.ZipCode
    ///
    /// Example of writing a value to a column in the row:
    ///  Row.ZipCode = zipCode
    /// </summary>
    /// <param name="Row">The row that is currently passing through the component</param>
    public override void Input0_ProcessInputRow(Input0Buffer Row)
    {
        string ARACID = Row.ARACID.ToString();

        string strCloseCode = string.Empty;
        string strCloseReason = string.Empty;
        if (!string.IsNullOrEmpty( Row.StatusReason))
        {
            strCloseCode = Row.StatusReasonCodes;
            strCloseReason = Row.StatusReason;
        }
        else
        {
            strCloseCode = Row.CancelReasonCodes;
            strCloseReason = Row.CancelReason;
        }

        CreateCloseRecord(ARACID);
        Create_CloseInfo2Clt_202Record(ARACID, strCloseCode, strCloseReason);
    }

    public override void CreateNewOutputRows()
    {
        /*
          Add rows by calling the AddRow method on the member variable named "<Output Name>Buffer".
          For example, call MyOutputBuffer.AddRow() if your output was named "MyOutput".
        */
    }

    private void Create_CloseInfo2Clt_202Record(string ARACID, string CloseCode, string CloseReason)
    {
        var record202 = new CloseInfoToClient_202()
        {
            AccountId = ARACID,
            DateCloseSentToClient = DateTime.Today,
            CloseCode = CloseCode,
            CloseReason = CloseReason
            //ClientRecallFileName = Variables.currentFile.ToString()
        };
        AddMaintenanceRow(record202);
    }

    private void CreateCloseRecord(string ARACID)
    {
        var recClose = new CloseAccount_C()
        {
            AccountId = ARACID,
            CloseCode = "RETURN"
        };
        AddMaintenanceRow(recClose);
    }
    
    private void AddMaintenanceRow<T>(T record) where T : GeneralPurposeRecordBase
    {
        var recordList = new List<T>();
        recordList.Add(record);
        var recordString = CommonEngine.WriteString<T>(recordList);
        MaintenanceBuffer.AddRow();
        MaintenanceBuffer.RowData = recordString.ToUpper();
    }

    //adding adjustment buffer
    private void AddAdjustmentRow<T>(T record) where T : GeneralPurposeRecordBase
    {
        var recordList = new List<T>();
        recordList.Add(record);
        var recordString = CommonEngine.WriteString<T>(recordList);
        AdjustmentBuffer.AddRow();
        AdjustmentBuffer.RowData = recordString.ToUpper();
    }

}
