    {
        var record201 = new ClientRecallInfo_201()
        {
            AccountId = ARACID,
            DateOfRecall = DateTime.Today,
            RecallCode = RecallCode,
            RecallReason = RecallReason
            //ClientRecallFileName = Variables.currentFile.ToString()
        };
        AddMaintenanceRow(record201);
    }

    private void CreateCloseRecord(string ARACID)
    {
        var recClose = new CloseAccount_C()
        {
            AccountId = ARACID,
            CloseCode = "RETURN"
        };
        AddMaintenanceRow(recClose);
    }

    private void CreatePayRecord(decimal CollecttAmtPaid, string ARACID, string TenderDate, decimal PrincipalPmtAmt, decimal InterestPmtAmt)
    {
        if (CollecttAmtPaid > 0)
        {
            iTransactionReference += iTransactionReference;
            var recordPAY = new Payment_PAY()
            {
                AccountId = ARACID,
                TransactionType = "DP",
                PaymentAmount = CollecttAmtPaid,
                TenderDate = Convert.ToDateTime(TenderDate),
                TransactionReference = iTransactionReference.ToString(),
                PrincipalPaymentAmount = PrincipalPmtAmt,
                InterestPaymentAmount = InterestPmtAmt
            };
            AddAdjustmentRow(recordPAY);
        }
    }

    private void CreateREVRecord(decimal CollecttAmtPaid, string ARACID, string TenderDate, string TransCode)
    {
        iTransactionReference += iTransactionReference;
        var recordREV = new PaymentReversal_REV()
        {
            AccountId = ARACID,
            ReversalType = TransCode,
            PaymentAmount = CollecttAmtPaid,
            TenderDate = Convert.ToDateTime(TenderDate)
        };
        AddAdjustmentRow(recordREV);
    }

    private void CreateADJRecord(decimal CollecttAmtPaid, string ARACID, string TenderDate, string TransCode, decimal PrincipalPmtAmt, decimal InterestPmtAmt)
    { 
        iTransactionReference += iTransactionReference;
       
        var recordPAY = new Adjustment_ADJ()
        {
            AccountId = ARACID,
            PrincipalAdjustmentAmount = PrincipalPmtAmt,
            InterestAdjustmentAmount = InterestPmtAmt,
            TransactionType = TransCode,
            TenderDate = Convert.ToDateTime(TenderDate),
            TransactionReference = iTransactionReference.ToString()
        };
        AddAdjustmentRow(recordPAY);
    }

    private void AddMaintenanceRow<T>(T record) where T : GeneralPurposeRecordBase
    {
        var recordList = new List<T>();
        recordList.Add(record);
        var recordString = CommonEngine.WriteString<T>(recordList);
        MaintenanceBuffer.AddRow();
        MaintenanceBuffer.RowData = recordString.ToUpper();
    }

    //adding adjustment buffer
    private void AddAdjustmentRow<T>(T record) where T : GeneralPurposeRecordBase
    {
        var recordList = new List<T>();
        recordList.Add(record);
        var recordString = CommonEngine.WriteString<T>(recordList);
        AdjustmentBuffer.AddRow();
        AdjustmentBuffer.RowData = recordString.ToUpper();
    }

}
