    public override void Input0_ProcessInputRow(Input0Buffer Row)
    {
        #region SQL Query
        if (conn.State != ConnectionState.Open)
            conn.Open();
        string db_ARACID = string.Empty;

        string query = @"SELECT TOP 1                                
                             LTRIM(RTRIM(ARACID)) AS ARACID
                            ,ARCLIENT.ARCLID 
                        FROM 
                            %inorder   
                            ARACCOUNT
                            JOIN ARCLIENT ON ARACCOUNT.ARACCLTID = ARCLIENT.ARCLID AND ARCLIENT.ARCLID IN ('DCLACH','DCLYDD') 
                        WHERE 
                            ARACCOUNT.ARACCLACCT = '" + Row.ClientReferenceNumber.Trim().ToString() + @"'";

        using (var command = new OdbcCommand(query, conn))
        {
            using (var reader = command.ExecuteReader())
            {
                if (reader.Read())
                {
                    //bAccountFound = true;
                    db_ARACID = reader.IsDBNull(0) ? "" : reader.GetString(0);
                }
                else
                {
                    //bAccountFound = false;
                }
            }
        }
        #endregion

        if (!string.IsNullOrEmpty(db_ARACID))
        {
            decimal dcPaymentAmount = 0;
            decimal dcPrincipalPmtAmt = 0;
            decimal dcInterestPmtAmt = 0;
            if (!string.IsNullOrEmpty(Row.PaymentAmount))
            {
                dcPaymentAmount = Convert.ToDecimal(Row.PaymentAmount);
            }
            if (!string.IsNullOrEmpty(Row.PrincipalPmtAmt))
            {
                dcPrincipalPmtAmt = Convert.ToDecimal(Row.PrincipalPmtAmt);
            }
            if (!string.IsNullOrEmpty(Row.InterestPmtAmt))
            {
                dcInterestPmtAmt = Convert.ToDecimal(Row.InterestPmtAmt);
            }

            string strTransTypeCode = Row.TransTypeCode.Trim().ToString();

            if (strTransTypeCode == "DP")
            {
               // if (dcPaymentAmount > 0)
              //  {
                    CreatePayRecord(dcPaymentAmount, db_ARACID, Row.TenderDate, dcPrincipalPmtAmt, dcInterestPmtAmt);
                //}
            }
            else if (strTransTypeCode =="DPNSF" || strTransTypeCode == "DPCOR")
            {
                CreateREVRecord(dcPaymentAmount, db_ARACID, Row.TenderDate, strTransTypeCode);
            }
            else if (strTransTypeCode == "DBJ" || strTransTypeCode == "CRJ")
            {
                CreateADJRecord(dcPaymentAmount, db_ARACID, Row.TenderDate, strTransTypeCode, dcPrincipalPmtAmt, dcInterestPmtAmt);
            }
        }
    }

