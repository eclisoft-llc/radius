    /// <param name="Row">The row that is currently passing through the component</param>
    public override void Input0_ProcessInputRow(Input0Buffer Row)
    {
        /*
         * Add your code here
         */

        try
        {
            string ssn = Row.SSN_IsNull ? "" : Row.SSN;
            string lname = Row.LastName_IsNull ? "" : Row.LastName;  
            string fname = Row.Fname_IsNull ? "" : Row.Fname;
            string mname = Row.MiddleName_IsNull ? "" : Row.MiddleName;

            filestr.AppendLine(ssn.PadRight(9) + Row.DOB.PadRight(8) + lname.PadRight(26) + fname.PadRight(20) + Row.ArtivaNumber.ToString().PadRight(28) + Row.ADStatusDate.PadRight(8) + mname.PadRight(20));
        } catch(Exception e)
        {
            bool cancel = false;
            ComponentMetaData.FireError(10, "Process Values", e.Message + "@@" + e.StackTrace, "", 0, out cancel);
        }
        }

}
