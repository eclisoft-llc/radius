    ///  Row.ZipCode = zipCode
    /// </summary>
    /// <param name="Row">The row that is currently passing through the component</param>
    public override void Input0_ProcessInputRow(Input0Buffer Row)
    {

        if (ishead)
        {
            outBuffer.AddRow();
            outBuffer.Data = "CUSTOMER ACCOUNT|FIRST NAME|MIDDLE NAME|LAST NAME|SUFFIX|SSN/ TAX ID #|ADDRESS|CITY|STATE|ZIP|CLIENT|AGREEMENT DATE|PRODUCT CODE|OCA Account Number";
            ishead = false;
        }

        string fname = Row.ARENFNM_IsNull ? "" : Row.ARENFNM;
        string lname = Row.ARENLNM_IsNull ? "" : Row.ARENLNM;
        string ssn = Row.ARENSSN_IsNull ? "" : Row.ARENSSN;
        string adr = Row.ARENADR_IsNull ? "" : Row.ARENADR;
        string cty = Row.ARENCTY_IsNull ? "" : Row.ARENCTY;
        string stt = Row.ARENST_IsNull ? "" : Row.ARENST;
        string zip = Row.ARENZIP_IsNull ? "" : Row.ARENZIP;

        string dline = Row.ARACCLACCT + "|" +
        fname + "|" +
         "|" +
        lname + "|" +
        "|" +
        ssn + "|" +
        adr + "|" +
        cty + "|" +
        stt + "|" +
        zip + "|";

        string client = "";
        string ad = Row.ZZACBKTEFFDTE_IsNull ? "" : Row.ZZACBKTEFFDTE.ToString("yyyyMMdd");
        //string pc = "";

        string[] a = { "ATTUC3", "ATTUCR", "ATTUCE", "ATTUHO" };
        string[] b = { "ATTUB3", "ATTUBO" };

        if (Row.ARCLID == "ATTMOB")
            client = "MB";
        else if (a.Contains(Row.ARCLID))
            client = "WC";
        else if (b.Contains(Row.ARCLID))
            client = "WB";
        /*
        if (!Row.ISACTIVE_IsNull)
        {

            if (Row.ISACTIVE == 1)
                pc = "01";
            else if (Row.ISACTIVE == 0)
                pc = "99";
        }
        */

        dline = dline + client + "|" + ad + "|99|" + Row.ARACID;

        outBuffer.AddRow();
        outBuffer.Data = dline;


    }

}
