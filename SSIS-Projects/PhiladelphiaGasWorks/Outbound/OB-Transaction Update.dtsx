using System;
using System.Data;
using Microsoft.SqlServer.Dts.Pipeline.Wrapper;
using Microsoft.SqlServer.Dts.Runtime.Wrapper;
using System.Collections.Generic;
using RGS.ArtivaGeneralPurpose.Data.Records;
using FileHelpers;
using System.Data.Odbc;
#endregion

[Microsoft.SqlServer.Dts.Pipeline.SSISScriptComponentEntryPointAttribute]
public class ScriptMain : UserComponent
{
    private bool _artivaFileCreated = false;
    OdbcConnection conn;

    public override void PreExecute()
    {
        base.PreExecute();
        conn = new OdbcConnection(Connections.Artiva.ConnectionString);
        conn.Open();
    }

    public override void PostExecute()
    {
        base.PostExecute();
        if (!_artivaFileCreated)
            System.IO.File.Delete(Connections.MaintenanceFile.ConnectionString);

        if (conn != null)
        {
            conn.Close();
            conn.Dispose();
        }
    }

    public override void Input0_ProcessInputRow(Input0Buffer Row)
    {
        string artivaID = "";

        #region SQL Query
        if (conn.State != ConnectionState.Open)
            conn.Open();

        string query = @"SELECT TOP 1
                                ARACID AS ArtivaAccountID                                
                                FROM ARCLIENT
                                INNER JOIN ARACCOUNT ON ARACCLTID = ARCLID AND ARCLID IN " + Variables.ClientIDs + @"
                                AND ISNULL(ARACCANCID, 'ACTIVE') NOT IN ('DUPE','RETURN')
                                AND ARACCOUNT.ZZACCREDINTCONSACNUM = '" + Row.dataid + @"'";

        using (var command = new OdbcCommand(query, conn))
        {
            using (var reader = command.ExecuteReader())
            {
                if (reader.Read())
                    artivaID = reader.GetDecimal(0).ToString();
            }
        }
        #endregion

        if (Row.trantype == "1001" || Row.trantype == "1002")
        {
            _artivaFileCreated = true;

            var rec221 = new SifPifCodeRptToClt_221
            {
                AccountId = artivaID,
                DateSifPifSentToClient = Row.trandate,
                SifOrPif = Row.trantype == "1001" ? "PIF" : "SIF",
                SifPifCodeSentInClientFile = Row.trantype,
                ClientOutboundFileName = Variables.currentFile
            };
            AddMaintenanceRow(rec221);
        }
    }

    private void AddMaintenanceRow<T>(T record) where T : GeneralPurposeRecordBase
    {
        var recordList = new List<T>();
        recordList.Add(record);
        var recordString = CommonEngine.WriteString<T>(recordList);
        MaintenanceBuffer.AddRow();
        MaintenanceBuffer.RowData = recordString.ToUpper();
    }
}]]></arrayElement>
