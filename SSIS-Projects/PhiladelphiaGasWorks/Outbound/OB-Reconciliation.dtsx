      <DTS:Variables />
      <DTS:PropertyExpression
        DTS:Name="[ODBC Source].[SqlCommand]">"SELECT DISTINCT 
	ARACCOUNT.ZZACCREDINTCONSACNUM AS data_id 
	,ARACCOUNT.ARACCLACCT pri_acctno 
	,IsNull(ZZACLSTATCDESENT,'101000') as status_code 
	,AFACCURBALINT AS data_bal_total 
	,SUM(CASE WHEN AFACBBALID = 'PRN' THEN AFACBCURBAL END) AS data_bal_principal 
	,'' AS data_bal_interest 
	,'' AS data_bal_court 
	,'' AS data_bal_attorney 
	,SUM(CASE WHEN AFACBBALID = 'ADCHG' THEN AFACBCURBAL END) AS data_bal_other 
	,'' AS fee_recovery 
FROM 
%STARTTABLE ARCLIENT
INNER JOIN ARACCOUNT ON  ARACCOUNT.ARACCLTID = ARCLIENT.ARCLID AND ARCLID  IN " + @[$Project::ClientIDs] + "
INNER JOIN ARENTITY ON ARACRPID = ARENID 
INNER JOIN AFACCOUNT ON AFACKEY = ARACFINACCTID 
INNER JOIN AFACCTBAL ON AFACKEY = AFACBACCTID 
	
INNER JOIN (    
    SELECT DISTINCT
        ZZACCREDINTCONSACNUM
        ,ARACCLACCT
        ,MAX(ARACLSTDTE) AS MAXLSTDTE
    FROM %STARTTABLE ARCLIENT
                INNER JOIN ARACCOUNT ON  ARACCOUNT.ARACCLTID = ARCLIENT.ARCLID AND ARCLID  IN " + @[$Project::ClientIDs] + "
     AND 
     (ARACCANCID IS NULL OR ARACCANCID NOT IN ('DUPE','RETURN'))
    GROUP BY 
        ZZACCREDINTCONSACNUM
        ,ARACCLACCT    
    )QRY on QRY.ZZACCREDINTCONSACNUM = ARACCOUNT.ZZACCREDINTCONSACNUM AND ARACCOUNT.ARACCLACCT = QRY.ARACCLACCT AND QRY.MAXLSTDTE = ARACCOUNT.ARACLSTDTE
WHERE 
	ARACCANCID is null OR ARACCANCID NOT IN ('DUPE','RETURN') 
GROUP BY 
    ARACCOUNT.ZZACCREDINTCONSACNUM
    ,ARCLID
    ,IsNull(ZZACLSTATCDESENT,'101000')"</DTS:PropertyExpression>
