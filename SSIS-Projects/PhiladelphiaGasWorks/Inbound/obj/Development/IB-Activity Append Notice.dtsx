using System;
using System.Data;
using Microsoft.SqlServer.Dts.Pipeline.Wrapper;
using Microsoft.SqlServer.Dts.Runtime.Wrapper;
using System.Collections.Generic;
using FileHelpers;
using RGS.ArtivaGeneralPurpose.Data.Records;
using System.Data.Odbc;
#endregion

[Microsoft.SqlServer.Dts.Pipeline.SSISScriptComponentEntryPointAttribute]
public class ScriptMain : UserComponent
{
    private bool _exceptionReportCreated = false;
    OdbcConnection conn;

    public override void PreExecute()
    {
        base.PreExecute();
        conn = new OdbcConnection(Connections.ArtivaConnection.ConnectionString);
        conn.Open();
    }

    public override void PostExecute()
    {
        base.PostExecute();
        if (!_exceptionReportCreated)
            System.IO.File.Delete(Connections.ExceptionReport.ConnectionString);

        if (conn != null)
        {
            conn.Close();
            conn.Dispose();
        }
    }

    public override void Input0_ProcessInputRow(Input0Buffer Row)
    {
        bool accountFound = false;
        string artivaID = "";

        #region SQL Query
        if (conn.State != ConnectionState.Open)
            conn.Open();

        string query = @"SELECT
                                ARACID AS ArtivaAccountID,
                                ARACCANCID,
                                ARACLPYDTE
                                FROM %STARTTABLE ARCLIENT
                                INNER JOIN ARACCOUNT ON ARCLID = ARACCLTID AND ARCLID IN " + Variables.ClientIDs + @"
                                WHERE ISNULL(ARACCANCID, 'ACTIVE') NOT IN ('DUPE','RETURN')
                                AND ARACCOUNT.ZZACCREDINTCONSACNUM = '" + Row.dataid + "'";

        using (var command = new OdbcCommand(query, conn))
        {
            using (var reader = command.ExecuteReader())
            {
                if (reader.Read())
                {
                    accountFound = true;
                    artivaID = reader.GetDecimal(0).ToString();
                }
            }
        }
        #endregion

        if (!accountFound)
        {
            _exceptionReportCreated = true;
            ExceptionReportBuffer.AddRow();
            ExceptionReportBuffer.Data = "ACCOUNT NOT FOUND IN ARTIVA,Creditor Internal Consumer Account Number from file:  " + Row.dataid;
            return;
        }

        var rec114 = new ClientActivityInformation_114
        {
            AccountId = artivaID,
            ActivityTypeId = Row.activitytypeid,
            ActivityItemDesc = Row.activitytypename,
            ActivityType = Row.activitytype,
            ActivityItem = Row.activityitem,
            ActivityComment = Row.activityitemdesc,
            ActivityDate = Row.activitydate_IsNull ? (DateTime?)null : Row.activitydate,
            ActivityAmount = string.IsNullOrWhiteSpace(Row.activityamount) ? string.Empty : Row.activityamount.Trim(),
            ActivityLastPayDate = Row.activitydue_IsNull ? (DateTime?)null : Row.activitydue,
            ActivityTotalPaymentAmount = Row.activityoutstanding_IsNull ? 0 : Row.activityoutstanding,
            ActivityArrangementType = Row.activitysource
        };

        var rec203s = new List<AccountFreeFormTextMntOnly_203>();
        Add203(artivaID, "PRODUCT TYPE:", Row.activityproduct, rec203s);
        Add203(artivaID, "ACTIVITY STATUS:", Row.activitystatus, rec203s);
        Add203(artivaID, "ACTIVITY LOCATION:", Row.activitylocation, rec203s);
        Add203(artivaID, "ACTIVITY METHOD:", Row.activitymethod, rec203s);
        Add203(artivaID, "ACTIVITY CODE:", Row.activitycode, rec203s);
        Add203(artivaID, "ACTIVITY CREDIT TYPE:", Row.activitycredittype, rec203s);

        AddMaintenanceRow(rec114);
        foreach (var rec203 in rec203s)
            AddMaintenanceRow(rec203);
    }

    private void Add203(string artivaID, string label, string data, List<AccountFreeFormTextMntOnly_203> list)
    {
        if (string.IsNullOrWhiteSpace(data))
            return;

        list.Add(new AccountFreeFormTextMntOnly_203 { AccountId = artivaID, FreeFormText = label + " " + data });
    }

    private void AddMaintenanceRow<T>(T record) where T : GeneralPurposeRecordBase
    {
        var recordList = new List<T>();
        recordList.Add(record);
        var recordString = CommonEngine.WriteString<T>(recordList);
        Output0Buffer.AddRow();
        Output0Buffer.RowData = recordString.ToUpper();
    }
}]]></arrayElement>
